<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorNeutral900">#FF1F1F1F</Color>
  <Color x:Key="ColorNeutral800">#FF212121</Color>
  <Color x:Key="ColorNeutral700">#FF424242</Color>
  <Color x:Key="ColorNeutral600">#FF616161</Color>
  <Color x:Key="ColorNeutral500">#FF9E9E9E</Color>
  <Color x:Key="ColorBorderSubtle">#FFDDDDDD</Color>
  <Color x:Key="ColorBorderDefault">#FFE0E0E0</Color>

  <Color x:Key="ColorInfo">#FF2F80ED</Color>
  <Color x:Key="ColorInfoStrong">#FF1565C0</Color>
  <Color x:Key="ColorInfoDark">#FF1976D2</Color>
  <Color x:Key="ColorInfoSurface">#FFE3F2FD</Color>
  <Color x:Key="ColorInfoBorder">#FFBBDEFB</Color>

  <Color x:Key="ColorSuccess">#FF27AE60</Color>
  <Color x:Key="ColorSuccessDark">#FF2E7D32</Color>
  <Color x:Key="ColorSuccessSurface">#FFE8F5E9</Color>
  <Color x:Key="ColorSuccessBorder">#FFC8E6C9</Color>

  <Color x:Key="ColorWarning">#FFF2994A</Color>
  <Color x:Key="ColorWarningText">#FF856404</Color>
  <Color x:Key="ColorWarningSurface">#FFFFF3CD</Color>
  <Color x:Key="ColorWarningBorder">#FFFFEEBA</Color>
  <Color x:Key="ColorWarningSurfaceSoft">#FFFFF3E0</Color>
  <Color x:Key="ColorWarningSurfaceMuted">#FFFFF8E1</Color>
  <Color x:Key="ColorWarningBorderStrong">#FFFFE082</Color>

  <Color x:Key="ColorDanger">#FFEB5757</Color>
  <Color x:Key="ColorDangerDark">#FFC62828</Color>
  <Color x:Key="ColorDangerText">#FFB71C1C</Color>
  <Color x:Key="ColorDangerAccent">#FFAD1457</Color>
  <Color x:Key="ColorDangerSurface">#FFFFEBEE</Color>
  <Color x:Key="ColorDangerBorder">#FFFFCDD2</Color>

  <Color x:Key="ColorOverlay">#80000000</Color>
  <Color x:Key="ColorSlate900">#FF263238</Color>
  <Color x:Key="ColorSlate800">#FF37474F</Color>
  <Color x:Key="ColorSlate700">#FF455A64</Color>
  <Color x:Key="ColorSlate600">#FF546E7A</Color>

  <Color x:Key="ColorRiskRead">#FF00897B</Color>
  <Color x:Key="ColorRiskFullControl">#FF9B51E0</Color>
  <Color x:Key="ColorRiskPath">#FF0D47A1</Color>
  <Color x:Key="ColorSurfaceAlt">#FFF5F5F5</Color>
  <Color x:Key="ColorSurfacePurple">#FFF3E5F5</Color>

  <SolidColorBrush x:Key="BrushNeutral900" Color="{StaticResource ColorNeutral900}" />
  <SolidColorBrush x:Key="BrushNeutral700" Color="{StaticResource ColorNeutral700}" />
  <SolidColorBrush x:Key="BrushNeutral600" Color="{StaticResource ColorNeutral600}" />
  <SolidColorBrush x:Key="BrushBorderSubtle" Color="{StaticResource ColorBorderSubtle}" />
  <SolidColorBrush x:Key="BrushInfo" Color="{StaticResource ColorInfo}" />
  <SolidColorBrush x:Key="BrushSuccess" Color="{StaticResource ColorSuccess}" />
  <SolidColorBrush x:Key="BrushWarning" Color="{StaticResource ColorWarning}" />
  <SolidColorBrush x:Key="BrushDanger" Color="{StaticResource ColorDanger}" />
</ResourceDictionary>
