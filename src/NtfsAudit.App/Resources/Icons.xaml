<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Style x:Key="IconText16" TargetType="TextBlock">
    <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
    <Setter Property="FontSize" Value="16" />
    <Setter Property="VerticalAlignment" Value="Center" />
    <Setter Property="Margin" Value="0,0,6,0" />
  </Style>

  <Style x:Key="IconScan16" TargetType="TextBlock" BasedOn="{StaticResource IconText16}">
    <Setter Property="Text" Value="&#xE721;" />
  </Style>
  <Style x:Key="IconStop16" TargetType="TextBlock" BasedOn="{StaticResource IconText16}">
    <Setter Property="Text" Value="&#xE71A;" />
  </Style>
  <Style x:Key="IconExport16" TargetType="TextBlock" BasedOn="{StaticResource IconText16}">
    <Setter Property="Text" Value="&#xEDE1;" />
  </Style>
  <Style x:Key="IconImport16" TargetType="TextBlock" BasedOn="{StaticResource IconText16}">
    <Setter Property="Text" Value="&#xE8B5;" />
  </Style>
  <Style x:Key="IconBrowse16" TargetType="TextBlock" BasedOn="{StaticResource IconText16}">
    <Setter Property="Text" Value="&#xE8B7;" />
  </Style>
</ResourceDictionary>
